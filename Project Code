    AFRAME.registerComponent('cursor-listener', {
        init: function () {
            const itemId = this.el.getAttribute('cursor-listener');

            this.el.addEventListener('click', () => {
                let link;
                if (itemId === 'Book') {
                    link = "https://www.amazon.in/gp/aw/d/9357311661/?encoding=UTF8&pd_rd_plhdr=t&aaxitk=86b1a8beff0d54c4398ebdf235c73ba2&hsa_cr_id=0&qid=1692177907&sr=1-1-e0fa1fdd-d857-4087-adda-5bd576b25987&ref=sbx_be_s_sparkle_mcd_asin_0_img&pd_rd_w=lU1lS&content-id=amzn1.sym.df9fe057-524b-4172-ac34-9a1b3c4e647d%3Aamzn1.sym.df9fe057-524b-4172-ac34-9a1b3c4e647d&pf_rd_p=df9fe057-524b-4172-ac34-9a1b3c4e647d&pf_rd_r=WGB0HF1SD2TD5KMES3M6&pd_rd_wg=yRQbL&pd_rd_r=5de34103-4394-4b64-a16a-8b1d204cf83e";
                } 
                else if (itemId === 'shirt') {
                    link = 'https://www.amazon.in/Allen-Solly-Sweatshirt-ASSTORGPN40121L_Grey-Melange/dp/B07DPMK4Z5/ref=sr_1_1_sspa?keywords=sweatshirts+for+men&qid=1692009864&sprefix=swshirts+for+men%2Caps%2C217&sr=8-1-spons&sp_csd=d2lkZ2V0TmFtZT1zcF9hdGY&psc=1';
                }
    
                if (link) {
                    window.open(link, '_blank');
                }
            });
        }
    });
</script>
